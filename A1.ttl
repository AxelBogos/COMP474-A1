@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix dbp: <http://dbpedia.org/property/> .
@prefix owl:<http://www.w3.org/2002/07/owl#> .

@prefix a1_schema: <http://a1.io/schema#> .
@prefix a1_data: <http://a1.io/data#> .

### Classes

a1_schema:University
  a rdfs:Class ;
  rdfs:subClassOf foaf:Organization ;
  rdfs:label "University"@en.

a1_schema:Lecture
	a rdfs:Class;
	rdfs:label "Lecture"@en.

a1_schema:ClassEvent
  a rdfs:Class;
  rdfs:label "Class Event"@en.

a1_schema:Lab
	a rdfs:Class;
	rdfs:subClassOf a1_schema:ClassEvent;
	rdfs:label "Laboratory"@en.

a1_schema:Tutorial
	a rdfs:Class;
	rdfs:subClassOf a1_schema:ClassEvent;
	rdfs:label "Tutorial"@en.

a1_schema:Course
	a rdfs:Class;
	rdfs:label "Course"@en.

a1_schema:Topic
	a rdfs:Class;
	rdfs:label "Topic"@en.

### Proprieties

a1_schema:PartOfClass
	a rdf:Property;
	rdfs:label "Part of class"@en ;
	rdfs:comment "A Lecture is part of a particular Class"@en ;
	rdfs:domain a1_schema:Lecture;
	rdfs:range a1_schema:Class.

a1_schema:PartOfLecture
	a rdf:Property;
	rdfs:label "Part of Lecture"@en ;
	rdfs:comment "A Class Event is part of a particular Lecture"@en ;
	rdfs:domain a1_schema:ClassEvent;
	rdfs:range a1_schema:Lecture.

a1_schema:OffersCourse
	a rdf:Property;
	rdfs:label "Offers"@en ;
	rdfs:comment "Classes a university offers"@en ;
	rdfs:domain a1_schema:Class;
	rdfs:range a1_schema:University.

a1_schema:CourseSubject
	a rdf:Property;
	rdfs:label "Course Subject"@en ;
	rdfs:comment "The Subject of a Course"@en ;
	rdfs:domain a1_schema:Course;
	rdfs:range rdfs:Literal.

a1_schema:CourseNumber
	a rdf:Property;
	rdfs:label "Course Number"@en ;
	rdfs:comment "The Number of a Course"@en ;
	rdfs:domain a1_schema:Course;
	rdfs:range rdfs:Literal.

a1_schema:LectureNumber
	a rdf:Property;
	rdfs:label "Lecture Number"@en ;
	rdfs:comment "Sequential number of a lecture"@en ;
	rdfs:domain a1_schema:Lecture;
	rdfs:range rdfs:Literal.

a1_schema:Outline
	a rdf:Property;
	rdfs:label "Course Outline"@en ;
	rdfs:comment "The Outline of a Course"@en ;
	rdfs:domain a1_schema:Course;
	rdfs:range rdfs:Literal.

a1_schema:Content
	a rdf:Property;
	rdfs:label "Content"@en ;
	rdfs:comment "Content of a class event"@en ;
	rdfs:domain a1_schema:ClassEvent;
	rdfs:range rdfs:Literal.

a1_schema:LectureOfClass
	a rdf:Property;
	rdfs:label "Lecture of Event"@en ;
	rdfs:comment "Lecture associated with a class"@en ;
	rdfs:domain a1_schema:Class;
	rdfs:range rdfs:Lecture.

#TODO: I think topics may actually be tied to specific lectures, rather than exclusively the course as a whole

a1_schema:TopicOf
	a rdf:Property;
	rdfs:label "Topics"@en ;
	rdfs:comment "Topics of a course"@en ;
	rdfs:domain a1_schema:Course;
	rdfs:range a1_schema:Topic.