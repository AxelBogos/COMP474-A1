@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> 
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> 
@prefix foaf: <http://xmlns.com/foaf/0.1/> 
@prefix dbr: <http://dbpedia.org/resource/> 
@prefix dbo: <http://dbpedia.org/ontology/>
@prefix dbp: <http://dbpedia.org/property/>
@prefix owl:<http://www.w3.org/2002/07/owl#>

@prefix a1_schema: <http://a1.io/schema#> 
@prefix a1_data: <http://a1.io/data#> 

a1_schema:University
  a rdfs:Class ;
  rdfs:subClassOf foaf:Organization ;
  rdfs:label "University"@en.

a1_schema:ClassEvent
  a rdfs:Class;
  rdfs:label "Class Event"@en.

a1_schema:Lecture
	a rdfs:Class;
	rdfs:subClassOf a1_schema:ClassEvent;
	rdfs:label "Lecutre"@en.

a1_schema:Lab
	a rdfs:Class;
	rdfs:subClassOf a1_schema:ClassEvent;
	rdfs:label "Laboratory"@en.

a1_schema:Tutorial
	a rdfs:Class;
	rdfs:subClassOf a1_schema:ClassEvent;
	rdfs:label "Tutorial"@en.

### Course
a1_schema:Course
	a rdfs:Class;
	rdfs:label "Course"@en.
a1_schema:Name
	a rdf:Property;
	rdfs:label "Course Name"@en ;
	rdfs:comment "The name of the Course"@en ;
	rdfs:domain a1_schema:Course;
	rdfs:range rdfs:Literal.
a1_schema:Subject
	a rdf:Property;
	rdfs:label "Course Subject"@en ;
	rdfs:comment "The Subject of the Course"@en ;
	rdfs:domain a1_schema:Course;
	rdfs:range rdfs:Literal.
a1_schema:Number
	a rdf:Property;
	rdfs:label "Course Number"@en ;
	rdfs:comment "The Number of the Course"@en ;
	rdfs:domain a1_schema:Course;
	rdfs:range rdfs:Literal.
a1_schema:Description
	a rdf:Property;
	rdfs:label "Course Description"@en ;
	rdfs:comment "The Description of the Course"@en ;
	rdfs:domain a1_schema:Course;
	rdfs:range rdfs:Literal.
a1_schema:Outline
	a rdf:Property;
	rdfs:label "Course Outline"@en ;
	rdfs:comment "The Outline of the Course"@en ;
	rdfs:domain a1_schema:Course;
	rdfs:range rdfs:Literal.
a1_schema:Link
	a rdfs:subPropertyOf rdfs:seeAlso


a1_schema:Topics
	a rdfs:Class;
	rdfs:label "Topic"@en.